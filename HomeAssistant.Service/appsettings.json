{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Grafana.Loki",
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "GrafanaLoki",
        "Args": {
          "uri": "http://localhost:3100",
          "labels": [
            {
              "key": "app",
              "value": "HomeAssistant.Service"
            }
          ],
          "propertiesAsLabels": [
            "app"
          ]
        }
      },
      {
        "Name": "Console"
      }
    ]
  },
  "HomeAssistantOptions": {
    "Token": "",
    "BaseURI": ""
  },
  "Vault": {
    "Address": "",
    "MountPath": ""
  }
}
